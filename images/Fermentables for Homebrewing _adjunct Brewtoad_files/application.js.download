BT.showModal = function(selector, options) {
  var defaults = {
    scroll: true,
    maxWidth: null,
    onShow: function(){}
  };

  var settings = $.extend({}, defaults, options);

  var $modal = $(selector);

  // Save scroll position
  $modal.data('oldScroll', $(window).scrollTop());

  // Scroll to top
  $(window).scrollTop(0);

  // Add modal background after (behind) the modal
  $modal.after('<div class="modal-background"></div>');

  // Wrap the modal in the modal container
  $modal.wrap('<div class="modal-container"></div>');

  // Set the max width of the modal container
  if(settings.maxWidth !== null) {
    $('.modal-container').css('max-width', settings.maxWidth);
  }

  // FIXME: This only seems to manifest on the recipe editor. Big hack!
  // The modal's initial top position is -10000px. Move it so it's visible.
  $('.modal-container').css('top', 0);

  // Show the modal
  $modal.show();

  if(settings.scroll === false) {
    // Add noscroll class to modal container
    $('.modal-container').addClass('noscroll');

    // Disable scrolling
    $('html').css('overflow-y', 'hidden');
  }

  // Set body height to modal height + padding or window height, whichever is greater
  var windowHeight = $(window).height();
  var modalHeight = $modal.height() + 64;
  var bodyHeight = windowHeight > modalHeight ? windowHeight : modalHeight;
  $('body').height(bodyHeight);

  // Hide overflow
  $('body').css('overflow-y', 'hidden');

  // Fade background in
  $('.modal-background').addClass('in');

  // Slide modal down
  $modal.addClass('in');

  // Close modal
  $(selector).on('click', '.close', function() {
    BT.hideModal(selector);
  });

  settings.onShow();
}

BT.hideModal = function(selector) {
  var $modal = $(selector);

  // Slide modal up
  $modal.removeClass('in');

  // Fade out modal background
  $('.modal-background').removeClass('in');

  setTimeout(function() {
    // Remove modal background
    $('.modal-background').remove();

    // Hide modal
    $modal.hide();

    // Unwrap modal
    $modal.unwrap();

    // Restore body height
    $('body').css('height', '');
    $('body').css('overflow-y', 'visible');

    // Restore scroll
    $(window).scrollTop($modal.data('oldScroll'));

    // FIXME: This only seems to manifest on the recipe editor. Big hack!
    // Hide the modal container so it doesn't cover up the page.
    $('.modal-container').css('top', -10000);

    // Reenable scrolling
    $('html').css('overflow-y', '');
  }, 50);
}

