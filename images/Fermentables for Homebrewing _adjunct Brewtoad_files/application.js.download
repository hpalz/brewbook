  */

  self.partialBoil = function() {
    return (self.fullBoilSize() > self.safeKettleVolume());
  }

  self.originalGravity = function() {
    return self.specificGravity(self.batchSize.normalized());
  }

  self.finalGravityForAttenuation = function(attenuation) {
    var og = self.originalGravity();
    if(og === 1) {
      return 1;
    }
    op = (og - 1) * 1000;
    fp = op * (1 - (attenuation / 100));
    return (fp / 1000) + 1;
  }

  self.finalGravity = function() {
    if(self.yeasts().length === 0) {
      return self.originalGravity();
    }
    return self.finalGravityForAttenuation(self.yeasts()[0].attenuation());
  }

  self.alcoholByVolume = function() {
    return (self.originalGravity() - self.finalGravity()) * 131
  }


  //==========================================================================
  // Strategy for choosing the correct IBU formula
  //==========================================================================

  self.estimatedIbu = function() {
    if(self.ibuFormulaId() === BT.Enum.IbuFormula.TINSETH) {
      return self.estimatedIbuTinseth();
    } else {
      return self.estimatedIbuRager();
    }
  }

  self.estimatedIbuForAdditionDisplay = function(alpha, amount, time, hopFormId, hopUseId) {
    if(self.ibuFormulaId() === BT.Enum.IbuFormula.TINSETH) {
      return self.estimatedIbuForAdditionTinseth(alpha, amount, time, hopFormId, hopUseId);
    } else {
      return self.estimatedIbuForAdditionRager(alpha, amount, time, hopFormId, hopUseId);
    }
  }

  //==========================================================================
  // IBU: Tinseth
  //==========================================================================

  self.estimatedIbuForAdditionTinseth = function(alpha, amount, time, hopFormId, hopUseId) {
    if(hopUseId === BT.Enum.HopUse.DRY_HOP || hopUseId === BT.Enum.HopUse.WHIRLPOOL) {
      return 0;
    }

    var ibu = 0;
    var aau = amount * (alpha / 100) * 100;
    var g = 1.65 * Math.pow(0.000125, self.boilSpecificGravity() - 1);
    var t = (1 - Math.pow(Math.E, -0.04 * time)) / 4.15;
    var u = g * t;

    if(hopFormId === BT.Enum.HopForm.LEAF) {
      u = u * 0.9;
    } else if(hopFormId == BT.Enum.HopForm.PLUG) {
      u = u * 0.92;
    }

    return aau * u * 75 / self.batchSize.normalized();
  }

  /*
  self.estimatedIbuForAdditionTinsethDisplay = function(alpha, amount, time, hopFormId) {
    if(typeof alpha === "undefined" || typeof amount === "undefined" || typeof time === "undefined" || typeof hopFormId === "undefined") {
      return "";
    }
    var ibu = self.ibuForAdditionTinseth(alpha, amount, time, hopFormId);
    return ibu.round(0);
