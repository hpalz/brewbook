BT.Users.FollowBrewer = function() {
  //
  // Changes the class & text of the element
  //
  function changeClassText($this) {
    $this.toggleClass('follow--link__active');

    if($this.hasClass('follow--link__active')){
      $this.find('i').removeClass('follow').addClass('followed');
      $this.find('.js-btn-text').text('Followed!');

    } else {
      $this.find('i').removeClass('followed').addClass('follow');
      $this.find('.js-btn-text').text('Follow');
    }

  }

  //
  // Follow
  //
  function follow($this) {

    $.ajax({
      async: true,
      dataType: 'json',
      type: 'POST',
      url: $this.attr('href') + '',
      success: function(r) {

      },
      error: function(r) {

        if(r.status !== 200) {
          $this.toggleClass('followed');
          $this.html('<span class="icon"/>Error!');
        }
      }
    });
  }

  //
  // Unfollow
  //
  function unfollow($this) {
    $.ajax({
      async: true,
      dataType: 'json',
      type: 'DELETE',
      url: $this.attr('href') + '',
      success: function(r) {

      },
      error: function(r) {

        if(r.status !== 200) {
          $this.toggleClass('followed');
          $this.html('Error!');
        }
      }
    });
  }

  //
  // Sets up the event handlers
  //
  function initUIBindings() {
    $('.js-follow').on('click', function(e) {
      e.preventDefault();
      changeClassText($(this));
      if($(this).hasClass('followed')) {
        unfollow($(this));
      } else {
        follow($(this));
      }
      $(this).toggleClass('followed')

    });
  }
  initUIBindings();
};

