BT.Shopping.EditOrder.OrderGroupIngredient = function(data) {
  var self = this;

  self.id                = ko.observable(data.id);
  self.name              = ko.observable(data.name);
  self.ingredientId      = ko.observable(data.ingredient_id);
  self.ingredientType    = ko.observable(data.ingredient_type);
  self.preferredVendorId = ko.observable(data.preferred_vendor_id);
  self.amount            = ko.observable(data.amount);
  self.amountUnit        = ko.observable(data.amount_unit);
  self.products          = ko.observableArray();
  self.constraints       = null;
  self.vendors           = ko.observableArray(data.vendors);

  if(typeof data.constraints !== 'undefined') {
    switch(self.ingredientType()) {
      case 'GenericFermentable':
        self.constraints = new BT.Shopping.EditOrder.OrderGroupIngredientFermentableConstraints(data.constraints);
        self.constraints.milled.subscribe(function(newValue) {
          self._updateProducts();
        });
        break;
      case 'Hop':
        self.constraints = new BT.Shopping.EditOrder.OrderGroupIngredientHopConstraints(data.constraints);
        self.constraints.hopFormId.subscribe(function(newValue) {
          self._updateProducts();
        });
        break;
    }
  }

  self.subtotal = ko.computed(function() {
    var subtotal = 0;
    ko.utils.arrayForEach(self.products(), function(product) {
      var price = parseFloat(product.totalPrice());
      if(!isNaN(price)) {
        subtotal += price;
      }
    });
    return subtotal;
  });

  self.subtotal.formatted = ko.computed(function() {
    var val = self.subtotal();

    if(isNaN(val)) {
      return '';
    } else {
      return '$' + val.toFixed(2);
    }
  });

  self._loadProducts = function(products) {
    products.forEach(function(product) {
      self.products.push(new BT.Shopping.EditOrder.OrderGroupProduct(product));
    });
  }

  self._updateProducts = function() {
    var data = {
      preferred_vendor_id: self.preferredVendorId() || null,
      ingredient_id:       self.ingredientId(),
      amount:              self.amount()
    };

    if(self.constraints !== null) {
      data = $.extend(data, { constraints: self.constraints.toJS() });
    }

    $.ajax('/shopping/order-group-ingredients/' + self.id(), {
      type: 'PUT',
      data: data,
      success: function(data) {
        self.products.removeAll();
        self._loadProducts(data.products);
        self.vendors(data.vendors);
      },
      error: function() {
        // FIXME: Better error handling
        alert('Error!');
      }
    });
  }

  self.amount.subscribe(function(newValue) {
    self._updateProducts();
  });

  self.preferredVendorId.subscribe(function(newValue) {
    self._updateProducts();
  });

  self.ingredientId.subscribe(function(newValue) {
    self._updateProducts();
  });

  self._loadProducts(data.products);

  return self;
};
