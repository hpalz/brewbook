  */

  self.estimatedIbuTinseth = function() {
    if(self.hops().length === 0) {
      return 0;
    }
    var ibu = 0;
    ko.utils.arrayForEach(self.hops(), function(h) {
      // Skip invalid rows
      if(!h.isValid()) {
        return;
      }
      ibu += self.estimatedIbuForAdditionTinseth(h.alpha(), h.amount.normalized(), h.time(), h.hopFormId(), h.hopUseId());
    });
    return ibu;
  }

  //==========================================================================
  // IBU: Rager
  //==========================================================================

  self.estimatedIbuForAdditionRager = function(alpha, amount, time, hopFormId, hopUseId) {
    if(hopUseId === BT.Enum.HopUse.DRY_HOP) {
      return 0;
    }

    utilization = {
      0:  5.0,
      1:  5.0,
      2:  5.0,
      3:  5.0,
      4:  5.0,
      5:  5.0,
      6:  6.0,
      7:  6.0,
      8:  6.0,
      9:  6.0,
      10: 6.0,
      11: 8.0,
      12: 8.0,
      13: 8.0,
      14: 8.0,
      15: 8.0,
      16: 10.1,
      17: 10.1,
      18: 10.1,
      19: 10.1,
      20: 10.1,
      21: 12.1,
      22: 12.1,
      23: 12.1,
      24: 12.1,
      25: 12.1,
      26: 15.3,
      27: 15.3,
      28: 15.3,
      29: 15.3,
      30: 15.3,
      31: 18.8,
      32: 18.8,
      33: 18.8,
      34: 18.8,
      35: 18.8,
      36: 22.8,
      37: 22.8,
      38: 22.8,
      39: 22.8,
      40: 22.8,
      41: 26.9
    };

    if(time < 0) {
      time = 0;
    }
    else if(time > 41) {
      time = 41;
    }

    u = utilization[time];

    var ga = 0;

    if(self.boilSpecificGravity() > 1.050) {
      ga = (self.boilSpecificGravity() - 1.050) / 0.2;
    }

    return (amount * (u / 100) * (alpha / 100.0) * 7489) / (self.boilSize() * (1 + ga));
  }

  /*
  self.estimatedIbuForAdditionRagerDisplay = function(alpha, amount, time, hopFormId) {
    if(typeof alpha === "undefined" || typeof amount === "undefined" || typeof time === "undefined" || typeof hopFormId === "undefined") {
      return "";
    }
    var ibu = self.ibuForAdditionTinseth(alpha, amount, time, hopFormId);
    return ibu.round(0);
  }
  */

  self.estimatedIbuRager = function() {
    if(self.hops().length === 0) {
      return 0;
    }
    var ibu = 0;
    ko.utils.arrayForEach(self.hops(), function(h) {
      // Skip invalid rows
      if(!h.isValid()) {
        return;
      }
      ibu += self.estimatedIbuForAdditionRager(h.alpha(), h.amount.normalized(), h.time(), h.hopFormId(), h.hopUseId());
    });
    return ibu;
  }

  //==========================================================================
  // Color: Morey
  //==========================================================================

  self.colorMorey = function() {
    if(!self.batchSize.isValid() || self.fermentables().length === 0) {
      return 0;
    }
    var mcu = 0;
    ko.utils.arrayForEach(self.fermentables(), function(f) {
      // Skip invalid rows
      if(!f.isValid()) {
        return;
      }
      mcu += (f.color() * f.amount.normalized()) / self.batchSize.normalized();
    });
    return 1.4922 * Math.pow(mcu, 0.6859);
  }


  //==========================================================================
  // Balance
  //==========================================================================

  self.balance = function() {
    if(self.estimatedIbu() === 0 && self.originalGravity() === 1) {
      return 0.5;
    } else if(self.estimatedIbu() === 0) {
      return 1.0;
    } else if(self.originalGravity() === 1) {
      return 0.0;
    } else {
      return self.estimatedIbu() / ((self.originalGravity() - 1) * 1000)
    }
  }


  //==========================================================================
  // Percent of total grist weight for a given addition
  //==========================================================================

  self.percentByWeight = function(weight) {
    if(isNaN(weight)) {
      return "0%";
    }

    return ((weight / self.fermentableWeight()) * 100).round(2) + "%";
  }


  //==========================================================================
  // Number of yeast cells needed to ferment the beer
  //==========================================================================

  self.numCells = function() {
    var yeast = self.yeasts()[0];
    if(typeof yeast === 'undefined') {
      return 0;
    }
    var yeastType = BT.yeastTypes.find(yeast.yeastTypeId());
    if(typeof yeastType === 'undefined') {
      return 0;
    }
    if(yeastType.name === 'Ale') {
      var cellCount = 750000;
    } else {
      var cellCount = 1500000;
    }
    var wortVolumeMl = BT.measurementSystem.convert(self.batchSize.normalized(), 'gal', 'ml');
    var measuredPlato = 259 - (259 / self.originalGravity());
    var numCells = parseFloat(cellCount * wortVolumeMl * measuredPlato) * 0.000000001;
    if(isNaN(numCells)) {
      return undefined;
    } else {
      return parseInt(numCells);
    }
  }

  self.numLiquid = function() {
    var ret = Math.round(self.numCells() / 100);
    if(isNaN(ret)) {
      return undefined;
    } else {
      return ret;
    }
  }

  self.gramsDry = function() {
    var ret = Math.round(self.numCells() / 20);
    if(isNaN(ret)) {
      return undefined;
    } else {
      return ret;
    }
  }

};
BT.MeasurementSystem = function(measurementSystemId) {
  var self = this;

  self.measurementSystemId  = ko.intObservable(measurementSystemId);

  self.isMetric = ko.computed(function() {
    return self.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC;
  })

  self.boilingTemp = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return 212;
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return 100;
        break;
    }
  });

  self.boilingTemp.normalized = ko.computed(function() {
    return 212;
  });

  self.grainWeightUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "lb";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "kg";
        break;
    }
  });

  self.hopWeightUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "oz";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "g";
        break;
    }
  });

  self.volumeUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "gal";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "L";
        break;
    }
  });

  self.evapRateUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "gal/hr";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "L/hr";
        break;
    }
  });

  self.temperatureUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "degF";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "degC";
        break;
    }
  });

  self.mashThicknessUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "qt/lb";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "L/kg";
        break;
    }
  });

  self.mashInfusionUnit = ko.computed(function() {
    switch(self.measurementSystemId()) {
      case BT.Enum.MeasurementSystem.US_CUSTOMARY:
        return "qt";
        break;
      case BT.Enum.MeasurementSystem.METRIC:
        return "L";
        break;
    }
  });

  self.convert = function(val, from, to, skipRounding) {
    if(typeof skipRounding === 'undefined') {
      skipRounding = false;
    }

    if(to === from) {
      return val;
    }

    var retVal;

    //
    // Weight
    //

    if(from === 'kg' && to === 'g') {
      retVal = val * 1000;
    }

    if(from === 'g' && to === 'kg') {
      retVal = val / 1000;
    }

    if(from === 'lb' && to === 'oz') {
      retVal = val * 16;
    }

    if(from === 'oz' && to === 'lb') {
      retVal = val / 16;
    }

    if(from === 'lb' && to === 'kg') {
      retVal = val * 0.453592;
    }

    if(from === 'kg' && to === 'lb') {
      retVal = val * 2.20462;
    }

    if(from === 'oz' && to === 'g') {
      retVal = val * 28.3495;
    }

    if(from === 'oz' && to === 'kg') {
      retVal = val / 35.274;
    }

    if(from === 'g' && to === 'oz') {
      retVal = val * 0.035274;
    }

    if(from === 'g' && to === 'lb') {
      retVal = val / 453.592;
    }

    //
    // Volume
    //

    if(from === 'gal' && to === 'L') {
      retVal = val * 3.78541;
    }

    if(from === 'gal' && to === 'ml') {
      retVal = val * 3785.41;
    }

    if(from === 'L' && to === 'gal') {
      retVal = val * 0.264172052;
    }

    if(from === 'qt' && to === 'gal') {
      retVal = val * 0.25;
    }

    if(from === 'gal' && to === 'qt') {
      retVal = val * 4;
    }

    if(from === 'qt' && to === 'L') {
      retVal = val * 0.946353;
    }

    if(from === 'L' && to === 'qt') {
      retVal = val * 1.05669;
    }

    //
    // Evaporation Rates
    //

    if(from === 'gal/hr' && to === 'L/hr') {
      retVal = val * 3.78541178;
    }

    if(from === 'L/hr' && to === 'gal/hr') {
      retVal = val * 0.264172052;
    }

    //
    // Temperatures
    //

    if(from === 'degF' && to === 'degC') {
      retVal = ((val - 32) * 5/9).round(1);
    }

    if(from === 'degC' && to === 'degF') {
      retVal = ((val * (9/5)) + 32).round(1);
    }

    //
    // Mash Thicknesses
    //

    if(from === 'qt/lb' && to === 'L/kg') {
      retVal = val * 2.08635112182;
    }

    if(from === 'L/kg' && to === 'qt/lb') {
      retVal = val * 0.479305707242;
    }

    //
    // Specialty Grain Usage Rates
    //

    if(from === 'oz/gal' && to === 'g/L') {
      retVal = val * 7.4891517;
    }

    if(from === 'g/L' && to === 'oz/gal') {
      retVal = val * 0.133526471;
    }

    if(retVal && skipRounding === false) {
      retVal = retVal.round(2)
    }

    return retVal;
  }

  return self;
};
BT.Calculators.AlcoholCalculator = function(args) {
  var self = this;

  function brixToGravity(brix) {
    brix = parseFloat(brix);

    if(isNaN(brix)) {
      return;
    }

    return (brix / (258.6 - (brix / 258.2 * 227.1))) + 1;
  }

  function gravityToBrix(gravity) {
    gravity = parseFloat(gravity);

    if(isNaN(gravity)) {
      return;
    }

    return (((182.4601 * gravity - 775.6821) * gravity + 1262.7794) * gravity - 669.5622);
  }

  function measuredBrixtoFinal() {
    oBrix = parseFloat($('#oBrix').val());
    mBrix = parseFloat($('#mBrix').val());

    if(isNaN(oBrix) || isNaN(mBrix)) {
      return;
    }

    var gravity = 1.001843 - 0.002318474 * oBrix - 0.000007775 * oBrix * oBrix - 0.000000034 * oBrix * oBrix * oBrix + 0.00574 * mBrix + 0.00003344 * mBrix * mBrix + 0.000000086 * mBrix * mBrix * mBrix;

    var fBrix = gravityToBrix(gravity);

    $('#fg').val(gravity.toFixed(3));
    $('#fBrix').val(fBrix.toFixed(2));
  }

  function calcAbv() {
    var og = parseFloat($('#og').val());
    var fg = parseFloat($('#fg').val());

    if(isNaN(og) || isNaN(fg)) {
      $('#abv').val('');
      return;
    }

    var abv = (og - fg) * 131;

    $('#abv').val(abv.toFixed(1) + ' %');

    calcAbw(abv);
  }

  function calcAbw(abv){
    var abw = abv * 0.79336;
    $('#abw').val(abw.toFixed(1) + ' %');

  }

  $('input').on('keyup', function(){
    var $this = $(this);
    if($this.attr('id') === 'oBrix'){
      $('#og').val(brixToGravity($this.val()).toFixed(3));
      measuredBrixtoFinal();
    }
    if($this.attr('id') === 'fBrix'){
      $('#fg').val(brixToGravity($this.val()).toFixed(3));
    }
    if($this.attr('id') === 'mBrix'){
      measuredBrixtoFinal();
    }
    if($this.attr('id') === 'og'){
      $('#oBrix').val(gravityToBrix($this.val()).toFixed(2));
      $('#mBrix').val('');
    }
    if($this.attr('id') === 'fg'){
      $('#fBrix').val(gravityToBrix($this.val()).toFixed(2));
      $('#mBrix').val('');
    }
    calcAbv();
  });
  $('input:first').focus();
};

