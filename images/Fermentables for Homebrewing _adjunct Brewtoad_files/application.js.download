BT.Recipes.Editor.RecipeFermentable = function(recipeView, attr) {
  var self = this;

  self.recipeView = ko.observable(recipeView);

  if(typeof attr === "undefined") {
    attr = {};
  } else {
    // When editing existing recipes, we need to convert either the lb or kg
    // representation of the amount as stored in the database to the desired
    // unit.
    if(attr.amount_is_metric === true) {
      attr.amount = BT.measurementSystem.convert(attr.amount, 'kg', attr.unit);
    } else {
      attr.amount = BT.measurementSystem.convert(attr.amount, 'lb', attr.unit);
    }
  }

  //
  // Instance Methods
  //

  self.amount               = ko.fermentableAmountObservable(attr.amount, attr.unit);
  self.name                 = ko.stringObservable(attr.name);
  self.genericFermentableId = ko.intObservable(attr.generic_fermentable_id);
  self.fermentableId        = ko.intObservable(attr.fermentable_id);
  self.ppg                  = ko.intObservable(attr.ppg);
  self.color                = ko.intObservable(attr.color);
  self.fermentableTypeId    = ko.intObservable(attr.fermentable_type_id || BT.fermentableTypes[0].id);
  self.maltsterId           = ko.intObservable(attr.maltster_id);
  self.fermentableUseId     = ko.intObservable(attr.recipe_fermentable_use_id);
  self.panelVisible         = ko.observable(false);

  //
  // Callbacks
  //

  self.fermentableTypeId.subscribe(function(newValue) {
    self.recalculateFermentableUse();
  });

  //
  // Instance Methods
  //

  self.fermentableTypeName = ko.computed(function() {
    var type = BT.fermentableTypes.find(self.fermentableTypeId());
    if(typeof type === "undefined") {
      return "";
    } else {
      return type.name;
    }
  });


  self.maltsterName = ko.computed(function() {
    var maltster = BT.maltsters.find(self.maltsterId());
    if(typeof maltster === "undefined") {
      return "";
    } else {
      return maltster.name;
    }
  });

  self.fermentableUse = ko.computed(function() {
    var fermentableUse = BT.fermentableUses.find(self.fermentableUseId());
    if(typeof fermentableUse === "undefined") {
      return "";
    } else {
      return fermentableUse.name;
    }
  });

  self.togglePanelVisibility = function() {
    self.panelVisible(!self.panelVisible());
  };

  self.recalculateFermentableUse = function() {
    self.fermentableUseId(self.calcFermentableUse());
  };

  self.calcFermentableUse = function() {
    var recipeTypeId = self.recipeView().recipeTypeId();
    var fermentableTypeId = self.fermentableTypeId();

    // Caramel malts are generally mashed in partial mash or all-grain recipes,
    // and may be steeped in extract recipes.
    if(fermentableTypeId === BT.Enum.FermentableType.CARAMEL_MALT) {
      if(recipeTypeId === BT.Enum.RecipeType.ALL_GRAIN || recipeTypeId === BT.Enum.RecipeType.PARTIAL_MASH) {
        return BT.Enum.RecipeFermentableUse.MASH;
      } else {
        return BT.Enum.RecipeFermentableUse.STEEP;
      }
    }

    // Base malts must be mashed to extract sugar.
    if(fermentableTypeId === BT.Enum.FermentableType.BASE_MALT) {
      if(recipeTypeId === BT.Enum.RecipeType.ALL_GRAIN || recipeTypeId === BT.Enum.RecipeType.PARTIAL_MASH) {
        return BT.Enum.RecipeFermentableUse.MASH;
      } else {
        return BT.Enum.RecipeFermentableUse.STEEP;
      }
    }

    // Kilned malts may be steeped in extract recipes, but will contribute negligable sugar.
    if(fermentableTypeId === BT.Enum.FermentableType.KILNED_MALT) {
      if(recipeTypeId === BT.Enum.RecipeType.ALL_GRAIN || recipeTypeId === BT.Enum.RecipeType.PARTIAL_MASH) {
        return BT.Enum.RecipeFermentableUse.MASH;
      } else {
        return BT.Enum.RecipeFermentableUse.STEEP;
      }
    }

    // Roasted malts are either steeped or mashed.
    if(fermentableTypeId === BT.Enum.FermentableType.ROASTED_MALT) {
      if(recipeTypeId === BT.Enum.RecipeType.ALL_GRAIN || recipeTypeId === BT.Enum.RecipeType.PARTIAL_MASH) {
        return BT.Enum.RecipeFermentableUse.MASH;
      } else {
        return BT.Enum.RecipeFermentableUse.STEEP;
      }
    }

    // Extracts are always boiled
    if(fermentableTypeId === BT.Enum.FermentableType.DRY_EXTRACT || fermentableTypeId === BT.Enum.FermentableType.LIQUID_EXTRACT || fermentableTypeId === BT.Enum.FermentableType.SUGAR) {
      return BT.Enum.RecipeFermentableUse.BOIL;
    }

    // Grains are mashed for partial mash and all grain, and steeped for extract
    if(fermentableTypeId === BT.Enum.FermentableType.GRAIN) {
      if(recipeTypeId === BT.Enum.RecipeType.ALL_GRAIN || recipeTypeId === BT.Enum.RecipeType.PARTIAL_MASH) {
        return BT.Enum.RecipeFermentableUse.MASH;
      } else {
        return BT.Enum.RecipeFermentableUse.STEEP;
      }
    }

    return null;
  };

  self.edit = function() {
    self.fermentableEditorView.show();
  }

  self.isValid = function() {
    return self.amount.isValid() && self.ppg.isValid() && self.color.isValid();
  }

  self.toJS = function() {
    return {
      name:                 self.name(),
      generic_fermentable_id: isNaN(self.genericFermentableId()) ? null : self.genericFermentableId(),
      fermentable_id:        isNaN(self.fermentableId()) ? null : self.fermentableId(),
      amount:               self.amount.normalizedForDb(),
      unit:                 self.amount.currentUnit(),
      ppg:                  self.ppg(),
      color:                self.color(),
      fermentable_type_id:    self.fermentableTypeId(),
      maltster_id:           self.maltsterId(),
      fermentable_use_id:     self.fermentableUseId()
    };
  }
};
BT.Recipes.Editor.RecipeHop = function(recipeView, attr) {
  var self = this;

  self.recipeView = ko.observable(recipeView);

  if(typeof attr === "undefined") {
    attr = {};
  }

  //
  // Properties
  //

  self.name         = ko.stringObservable(attr.name);
  self.hopId        = ko.intObservable(attr.hop_id);
  self.amount       = ko.measuredValueObservable(attr.amount, "oz", "g");
  self.time         = ko.intObservable(attr.time);
  self.hopUseId     = ko.intObservable(attr.hop_use_id || BT.Enum.HopUse.BOIL);
  self.hopFormId    = ko.intObservable(attr.hop_form_id || BT.Enum.HopForm.PELLET);
  self.countryId    = ko.intObservable(attr.country_id);
  self.alpha        = ko.floatObservable(attr.alpha);
  self.panelVisible = ko.observable(false);

  self.countryCode = ko.computed(function() {
    var obj = BT.countries.find(self.countryId());
    if(typeof obj !== "undefined") {
      return obj.code;
    }
    return "";
  });

  //
  // Instance Methods
  //

  self.togglePanelVisibility = function() {
    self.panelVisible(!self.panelVisible());
  };

  self.edit = function() {
    BT.hopEditorView.show(self);
  }

  self.isValid = function() {
    return self.amount.isValid() && self.time.isValid() &&
           self.hopUseId.isValid() && self.hopFormId.isValid() &&
           self.alpha.isValid();
  }

  self.calcHopUseUnit = function(){
    if(self.hopUseId() === BT.Enum.HopUse.DRY_HOP){
      return 'days';
    }
    return 'min';
  }

  //
  // Serialization
  //

  self.toJS = function() {
    return {
      name:      self.name(),
      hop_id:     isNaN(self.hopId()) ? null : self.hopId(),
      amount:    self.amount(),
      time:      self.time(),
      hop_use_id:  self.hopUseId(),
      hop_form_id: self.hopFormId(),
      country_id: self.countryId(),
      alpha:     self.alpha()
    };
  }
};
