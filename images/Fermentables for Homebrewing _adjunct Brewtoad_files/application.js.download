BT.EmulatedRecipe = function(args) {
  var self = this;

  //
  // Properties
  //

  self.ibuFormulaId    = args.ibuFormulaId;
  self.fermentables    = args.fermentables;
  self.hops            = args.hops;
  self.yeasts          = args.yeasts;
  self.recipeTypeId    = args.recipeTypeId;
  self.batchSize       = args.batchSize;
  self.evapRate        = args.evapRate;
  self.boilTime        = args.boilTime;
  self.efficiency      = args.efficiency;
  self.kettleVolume    = args.kettleVolume;
  self.trubChillerLoss = args.trubChillerLoss;

  //
  // Instance Methods
  //

  // Max safe volume in kettle
  self.safeKettleVolume = function() {
    // 90% of kettle volume
    return self.kettleVolume() * 0.9;
  };

  self.fermentableWeight = function() {
    w = 0;
    ko.utils.arrayForEach(self.fermentables(), function(f) {
      if(typeof f.amount() === 'string' && f.amount() === '') {
        return 0
      }
      if(typeof f.amount() !== "undefined") {
        w += f.amount.normalized();
      }
    });
    return w;
  }

  self.specificGravity = function(volume) {
    if(!self.recipeTypeId.isValid() || self.fermentables().length === 0) {
      return 1;
    }

    var pts = 0;

    ko.utils.arrayForEach(self.fermentables(), function(f) {
      // Skip invalid rows
      if(!f.isValid()) {
        return;
      }

      // Grains which aren't already converted don't contribute gravity in extract recipes
      if(self.recipeTypeId() === BT.Enum.RecipeType.EXTRACT &&
         (f.fermentableTypeId() === BT.Enum.FermentableType.GRAIN       ||
          f.fermentableTypeId() === BT.Enum.FermentableType.ADJUNCT     ||
          f.fermentableTypeId() === BT.Enum.FermentableType.BASE_MALT   ||
          f.fermentableTypeId() === BT.Enum.FermentableType.KILNED_MALT ||
          f.fermentableTypeId() === BT.Enum.FermentableType.ROASTED_MALT)) {
        return;
      }

      // Calculate point contribution
      var pointContribution = f.ppg() * f.amount.normalized();

      var efficiency = self.efficiency();

      // Assume 75% extract efficiency of pre-converted grains
      // in extract recipes or when mash efficiency is unavailable.
      if(typeof efficiency === "undefined" || isNaN(efficiency))
        efficiency = 75;

      // Apply efficiency for mashed grains
      if(self.recipeTypeId() !== BT.Enum.RecipeType.EXTRACT &&
         f.fermentableTypeId() === BT.Enum.FermentableType.GRAIN        ||
         f.fermentableTypeId() === BT.Enum.FermentableType.ADJUNCT      ||
         f.fermentableTypeId() === BT.Enum.FermentableType.BASE_MALT    ||
         f.fermentableTypeId() === BT.Enum.FermentableType.KILNED_MALT  ||
         f.fermentableTypeId() === BT.Enum.FermentableType.CARAMEL_MALT ||
         f.fermentableTypeId() === BT.Enum.FermentableType.ROASTED_MALT) {
        pointContribution *= (efficiency / 100);
      }

      pts += pointContribution * (1 / volume);
    });

    return (pts / 1000) + 1;
  }

  self.boilSpecificGravity = function() {
    return self.specificGravity((self.boilSize() + self.batchSize.normalized()) / 2);
  }

  self.fullBoilSize = function() {
    if(!self.batchSize.isValid() || !self.evapRate.isValid() || !self.boilTime.isValid() || !self.trubChillerLoss.isValid()) {
      return 0;
    }

    var amountEvaporated = self.evapRate() * (self.boilTime() / 60);

    return self.batchSize.normalized() + amountEvaporated + self.trubChillerLoss.normalized();
  };

  self.topUpWater = function() {
    if(self.partialBoil()) {
      return self.fullBoilSize() - self.safeKettleVolume();
    } else {
      return 0;
    }
  };

  self.topUpWater.denormalized = function() {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
      return BT.measurementSystem.convert(self.topUpWater(), "gal", "L");
    } else {
      return self.topUpWater();
    }
  }

  self.boilSize = function() {
    if(self.partialBoil()) {
      return self.safeKettleVolume();
    } else {
      return self.fullBoilSize();
    }
  };

  self.boilSize.denormalized = function() {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
      return BT.measurementSystem.convert(self.boilSize(), "gal", "L");
    } else {
      return self.boilSize();
    }
  }

  /*
  self.minimumSafeKettleVolume = function() {
    if(self.batchSize.isInvalid() || self.evapRate.isInvalid() || self.boilTime.isInvalid() || self.trubChillerLoss.isInvalid()) {
      return "";
    }

    var fudge = 0.1; // 10% safety factor
    return ((self.batchSize() + (self.evapRate() * (self.boilTime() / 60)) + self.trubChillerLoss()) * (1 + fudge)).toFixed(2);
  };

  self.minimumSafeKettleVolume.denormalized = function() {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
