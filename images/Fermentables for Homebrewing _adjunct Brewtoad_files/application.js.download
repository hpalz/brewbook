BT.Recipes.Editor.RecipeModel = function() {
  var self = this;

  self.bind = function() {
    // Apply bindings for recipe view
    ko.applyBindings(
      BT.recipeView,
      document.getElementById("recipe-view")
    );

    $("#loading-view").hide();
    $("#recipe-view").fadeIn();

    // Triggering a scroll on the window causes the sidebar position to initialize
    $(window).scroll();
  }

  self.save = function() {
    BT.recipeView.validationErrors.removeAll();

    $('.btn--publish').html('<i class="icon ion-looping" /> Saving Recipe...');
    $('.btn--publish').attr('disabled', 'disabled');

    if(BT.recipeView.slug.isValid()) {
      var url = "/api/v1/recipes/" + BT.recipeView.slug();
      $.ajax(url, {
        type: "PUT",
        data: {
          "api_key": BT.apiKey,
          "recipe":  BT.recipeView.toJS()
        },
        error: function(jqXHR, textStatus, errorThrown) {
          try {
            BT.recipeView.validationErrors(JSON.parse(jqXHR.responseText).errors);
          } catch(err) {
            alert('There was an error saving your recipe. Please try again.');
          }

          $(window).scrollTop(0);

          $('.btn--publish').html('Save Recipe');
          $('.btn--publish').removeAttr('disabled');
        },
        success: function(data, textStatus, jqXHR) {
          BT.recipeView.slug(data.slug);
          window.location = "/recipes/" + BT.recipeView.slug();
          //history.replaceState(null, "Edit Recipe | Brewtoad", "/recipes/" + BT.recipeView.slug() + "/edit");
          //$('.btn--publish').text('Recipe Saved!');
          //setTimeout(function() {
          //  $('.btn--publish').text('Save Recipe');
          //}, 2000);
        }
      });
    } else {
      $.ajax("/api/v1/recipes", {
        type: "POST",
        data: {
          "api_key": BT.apiKey,
          "recipe":  BT.recipeView.toJS()
        },
        error: function(jqXHR, textStatus, errorThrown) {
          BT.recipeView.validationErrors(JSON.parse(jqXHR.responseText).errors);

          $(window).scrollTop(0);
          
          $('.btn--publish').html('Save Recipe');
          $('.btn--publish').removeAttr('disabled');
        },
        success: function(data, textStatus, jqXHR) {
          BT.recipeView.slug(data.slug);
          window.location = "/recipes/" + BT.recipeView.slug();
          //history.replaceState(null, "Edit Recipe | Brewtoad", "/recipes/" + BT.recipeView.slug() + "/edit");
        }
      });
    }
  };

  // Initialize
  BT.recipeView = new BT.Recipes.Editor.Recipe(BT.recipeFromServer);
  self.bind();

  return self;
};
