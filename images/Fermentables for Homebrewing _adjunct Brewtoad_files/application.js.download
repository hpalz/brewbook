Number.prototype.round = function(decimals) {
  return Math.round(this * Math.pow(10, decimals)) / Math.pow(10, decimals);
}

String.prototype.truncate = function(length) {
  if (this.length > length)
    return this.substring(0, length) + "...";
  else
    return this;
}

String.prototype.indefiniteArticlerize = function() {
  return ["a", "e", "i", "o", "u"].indexOf(this[0].toLowerCase()) === 0 ? "an " + this : "a " + this;
}

String.prototype.capitalizeFirstLetter = function() {
  return this[0].toUpperCase() + this.slice(1);
}

String.prototype.supplant = function (o) {
    return this.replace(/{([^{}]*)}/g,
        function (a, b) {
            var r = o[b];
            return typeof r === 'string' || typeof r === 'number' ? r : a;
        }
    );
};

Number.prototype.asGravity = function() {
  return this.toFixed(3);
}

Number.prototype.asTemperature = function() {
  return this.toFixed() + "&nbsp;&deg;F";
}

Number.prototype.asSRM = function() {
  return this.toFixed() + "&nbsp;SRM";
}

Number.prototype.asColorClass = function() {
  var val = this > 40 ? 40 : this;
  return 'srm' + val.toFixed();
}

Number.prototype.asPercent = function(num) {
  return this.toFixed(num) + "%";
}

Number.prototype.asGallons = function() {
  return this.toFixed(2) + "&nbsp;gal";
}

Number.prototype.asABV = function() {
  return this.toFixed(1) + "%";
}

Number.prototype.asIBU = function() {
  return this.toFixed();
}

ko.floatObservable = function(initialValue) {
  if(typeof initialValue !== "undefined") {
    initialValue = parseFloat(initialValue);
  }
  var _actual = ko.observable(initialValue);

  var result = ko.dependentObservable({
    read: function() {
      return _actual();
    },
    write: function(value) {
      var parsedValue = parseFloat(value);
      _actual(isNaN(parsedValue) ? undefined : parsedValue);
    }
  });

  result.isValid = ko.computed(function() {
    return typeof result() !== "undefined" && !isNaN(result());
  });

  result.isInvalid = ko.computed(function() {
    return !result.isValid();
  });

  return result;
};

ko.intObservable = function(initialValue) {
  if(typeof initialValue !== "undefined") {
    initialValue = parseInt(initialValue);
  }
  var _actual = ko.observable(initialValue);

  var result = ko.dependentObservable({
    read: function() {
      return _actual();
    },
    write: function(value) {
      var parsedValue = parseInt(value);
      _actual(isNaN(parsedValue) ? undefined : parsedValue);
    }
  });

  result.isValid = ko.computed(function() {
    return typeof result() !== "undefined" && !isNaN(result());
  });

  result.isInvalid = ko.computed(function() {
    return !result.isValid();
  });

  return result;
};

ko.stringObservable = function(initialValue) {
  var _actual = ko.observable(initialValue);

  var result = ko.dependentObservable({
    read: function() {
      return _actual();
    },
    write: function(value) {
      _actual(value);
    }
  });

  result.isValid = ko.computed(function() {
    return typeof result() !== "undefined" && result() !== "" && result() !== null;
  });

  result.isInvalid = ko.computed(function() {
    return !result.isValid();
  });

  return result;
};

ko.measuredValueObservable = function(val, usCustomaryUnit, metricUnit) {
  var _actual = ko.floatObservable(val);
  var _usCustomaryUnit = usCustomaryUnit;
  var _metricUnit = metricUnit;
  var _measurementSystemId = BT.measurementSystem.measurementSystemId();

  function _convert() {
    if(_measurementSystemId !== BT.measurementSystem.measurementSystemId()) {
      // US Customary to Metric
      if(_measurementSystemId === BT.Enum.MeasurementSystem.US_CUSTOMARY) {
        _actual(BT.measurementSystem.convert(_actual(), _usCustomaryUnit, _metricUnit));
        _measurementSystemId = BT.Enum.MeasurementSystem.METRIC;
      }
      // Metric to US Customary
      else {
        _actual(BT.measurementSystem.convert(_actual(), _metricUnit, _usCustomaryUnit));
        _measurementSystemId = BT.Enum.MeasurementSystem.US_CUSTOMARY;
      }
    }
  }

  var result = ko.computed({
    read: function() {
      _convert();
      return _actual();
    },
    write: function(value) {
      _measurementSystemId = BT.measurementSystem.measurementSystemId();
      _actual(value);
    }
  });

  result.unit = ko.stringObservable();

  result.availableUnits = ko.computed(function() {
    _convert();
    if(_measurementSystemId === BT.Enum.MeasurementSystem.METRIC) {
      return [{name:"kg"}, {name:"g"}];
    } else {
      return [{name:"lb"}, {name:"oz"}];
    }
  });

  result.normalized = ko.computed(function() {
    _convert();
    if(_measurementSystemId === BT.Enum.MeasurementSystem.METRIC) {
      return BT.measurementSystem.convert(_actual(), _metricUnit, _usCustomaryUnit);
    } else {
      return _actual();
    }
  });

  result.isValid = ko.computed(function() {
    return typeof result() !== "undefined" && !isNaN(result());
  });

  result.isInvalid = ko.computed(function() {
    return !result.isValid();
  });

  return result;
};

ko.fermentableAmountObservable = function(val, currentUnit) {
  var _actual = ko.observable(val);
  var _currentMeasurementSystemId = BT.measurementSystem.measurementSystemId();
  var _currentUnit = ko.observable(currentUnit);

  var result = ko.computed({
    read: function () {
      return _actual();
    },
    write: function (value) {
      _currentMeasurementSystemId = BT.measurementSystem.measurementSystemId();
      _actual(value);
    }
  });

  result.currentUnit = ko.computed({
    read: function () {
      return _currentUnit();
    },
    write: function (value) {
      _actual(BT.measurementSystem.convert(_actual(), _currentUnit(), value));
      _currentUnit(value);
    }
  });

  result.availableUnits = ko.computed(function () {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
      return [{
        name: 'kg'
      }, {
        name: 'g'
      }];
    } else {
      return [{
        name: 'lb'
      }, {
        name: 'oz'
      }];
    }
  });

  result.normalized = ko.computed(function() {
    if (_currentUnit() !== 'lb') {
      return parseFloat(BT.measurementSystem.convert(_actual(), _currentUnit(), 'lb'));
    } else {
      return parseFloat(_actual());
    }
  });

  result.normalizedForDb = ko.computed(function() {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
      if(_currentUnit() !== 'kg') {
        return parseFloat(BT.measurementSystem.convert(_actual(), _currentUnit(), 'kg', true));
      }
    } else {
      if(_currentUnit() !== 'lb') {
        return parseFloat(BT.measurementSystem.convert(_actual(), _currentUnit(), 'lb', true));
      }
    }
    return parseFloat(_actual());
  });

  result.isValid = ko.computed(function() {
    var r = parseFloat(result());
    return typeof r !== 'undefined' && !isNaN(r);
  });

  result.isInvalid = ko.computed(function() {
    return !result.isValid();
  });

  return result;
};

BT.usageRateForBatch = function(ozPerGal, batchSize) {
  if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
    var gPerL = BT.measurementSystem.convert(ozPerGal, 'oz/gal', 'g/L');
    var g = gPerL * batchSize;
    if(g >= 1000) {
      return (g / 1000).round(1) + ' kg';
    } else {
      return g.round(0) + ' g';
    }
  } else {
    oz = ozPerGal * batchSize;
    if(oz >= 16) {
      return (oz / 16).round(1) + ' lb';
    } else {
      return oz.round(1) + ' oz';
    }
  }
}

