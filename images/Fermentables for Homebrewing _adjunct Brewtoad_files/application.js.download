BT.BrewLogs.Form = function() {
  var self = this;

  function addRow($list, association, template) {
    var new_id = new Date().getTime(),
        regexp = new RegExp("new_" + association, "g"),
        row    = template.replace(regexp, new_id);

    var $row = $(row);

    $row.find('[type=datetime-local]').val(strftime('%Y-%m-%dT%H:%M'));

    $list.prepend($row);

    return $row;
  }

  function deleteRow($row) {
    $row.addClass('deleted').find('input.destroy').val(1);
    $row.hide();
  }

  function activeRow() {
    return $(this).find('.destroy').val() !== '1'
  }

  $('ol').on('click', '.js-delete-row', function(e) {
    e.preventDefault();
    var $this = $(this);
    var $row = $this.parents('li');
    deleteRow($row);
  });

  $('.js-add-row').click(function(e) {
    e.preventDefault();

    var $list = $(this).closest('.soft').find('ol');
    var association = $list.attr('id');
    var rowTemplate = decodeURIComponent($list.attr('data-row-template'));

    addRow($list, association, rowTemplate);

    return false;
  });

  $('.js-add-reminder-row').click(function(e) {
    e.preventDefault();

    var reminderType = $(this).attr('data-reminder-type');
    var $list = $(this).closest('.soft').find('ol');
    var association = "brew_log_reminders";
    var rowTemplate = decodeURIComponent($list.attr('data-row-template'));

    var $row = addRow($list, association, rowTemplate);

    $row.find('[type=date]').val(strftime('%Y-%m-%d'));
    $row.find('.reminder-type').val(reminderType);

    return false;
  });

  function toggleSaveAsGroup() {
    if($("input#save-as-group").is(":checked")) {
      $("#group-select").show();
    } else {
      $("#group-select").hide();
      $("#group-select select").val("");
    }
  }

  $("input#save-as-group").change(function() {
    toggleSaveAsGroup();
  });

  // Display a warning when the brew date is changed
  $("#brew_log_brew_date").change(function() {
    $(".brew-date-warning").show();
    $("#reminders-form").hide();
  });

  toggleSaveAsGroup();

  function calcEvaporationRate() {
    var boilTime         = parseFloat($('#brew_log_boil_time').val());
    var preBoilVol       = parseFloat($('#brew_log_pre_boil_volume').val());
    var postBoilVol      = parseFloat($('#brew_log_post_boil_volume').val());

    if(isNaN(preBoilVol) || isNaN(postBoilVol) || isNaN(boilTime)) {
      $('#evaporation-rate').html('&mdash;');
      $('#brew_log_calculated_evaporation_rate').val('');
      return;
    };

    var preBoilVolUnits  = $('#brew_log_pre_boil_volume_unit').val();
    var preBoilQty       = new Qty(preBoilVol + ' ' + preBoilVolUnits);
    var preBoilVolInGal  = preBoilQty.to('gal');

    var postBoilVolUnits = $('#brew_log_post_boil_volume_unit').val();
    var postBoilQty      = new Qty(postBoilVol + ' ' + postBoilVolUnits);
    var postBoilVolInGal = postBoilQty.to('gal');

    var evaporationRate = (((preBoilVolInGal.scalar - postBoilVolInGal.scalar) / (boilTime / 60)) / preBoilVolInGal.scalar) * 100;

    $('#evaporation-rate').html(evaporationRate.toFixed(1) + ' %');
    $('#brew_log_calculated_evaporation_rate').val(evaporationRate);
  }

  calcEvaporationRate();

  $('#brew_log_pre_boil_volume, #brew_log_boil_time, #brew_log_post_boil_volume').keyup( function(){
    calcEvaporationRate();
  });

  $('#brew_log_pre_boil_volume_unit, #brew_log_post_boil_volume_unit').change( function() {
    calcEvaporationRate();
  });

  function calcAbv() {
    var og = parseFloat($('#brew_log_og').val());
    var fg = parseFloat($('#brew_log_fg').val());

    if(isNaN(og) || isNaN(fg)) {
      $('#abv').html('&ndash;');
      $('#brew_log_calculated_abv').val('');
      return;
    }

    var abv = (og - fg) * 131;

    $('#abv').html(abv.toFixed(1) + ' %');
    $('#brew_log_calculated_abv').val(abv);
  };

  calcAbv();

  $('#brew_log_og, #brew_log_fg').keyup(function() {
    calcAbv();
  });
};
BT.Shopping.EditOrder.Order = function(data) {
  var self = this;

  self.groups = ko.observableArray();

  self._load = function(data) {
    self.vendors = data.vendors;
    self.fermentables = data.fermentables;
    self.hops = data.hops;
    self.yeasts = data.yeasts;
    self.extras = data.extras;
    self.hopForms = data.hopForms;

    data.order.groups.forEach(function(group) {
      self.groups.push(new BT.Shopping.EditOrder.OrderGroup(group));
    });
  };

  self.subtotal = ko.computed(function() {
    var subtotal = 0;
    ko.utils.arrayForEach(self.groups(), function(group) {
      var price = parseFloat(group.subtotal());
      if(!isNaN(price)) {
        subtotal += price;
      }
    });

    return subtotal;
  });

  self.subtotal.formatted = ko.computed(function() {
    var val = self.subtotal();

    if(isNaN(val)) {
      return '';
    } else {
      return '$' + val.toFixed(2);
    }
  });

  self._load(data);
  ko.applyBindings(self, $('.order')[0]);

  return self;
};
BT.Shopping.EditOrder.OrderGroup = function(data) {
  var self = this;

  self.id           = ko.observable(data.id);
  self.name         = ko.observable(data.name);
  self.fermentables = ko.observableArray();
  self.hops         = ko.observableArray();
  self.yeasts       = ko.observableArray();
  self.extras       = ko.observableArray();
  self.products     = ko.observableArray();

  self._load = function(data) {
    data.fermentables.forEach(function(ingredient) {
      self.fermentables.push(new BT.Shopping.EditOrder.OrderGroupIngredient(ingredient));
    });

    data.hops.forEach(function(ingredient) {
      self.hops.push(new BT.Shopping.EditOrder.OrderGroupIngredient(ingredient));
    });

    data.yeasts.forEach(function(ingredient) {
      self.yeasts.push(new BT.Shopping.EditOrder.OrderGroupIngredient(ingredient));
    });

    data.extras.forEach(function(ingredient) {
      self.extras.push(new BT.Shopping.EditOrder.OrderGroupIngredient(ingredient));
    });

    data.products.forEach(function(product) {
      self.products.push(new BT.Shopping.EditOrder.OrderGroupProduct(product));
    });
  };

  self.subtotal = ko.computed(function() {
    var ingredients = self.fermentables().concat(self.hops().concat(self.yeasts().concat(self.extras())));
    var subtotal = 0;
    ko.utils.arrayForEach(ingredients, function(ingredient) {
      var price = parseFloat(ingredient.subtotal());
      if(!isNaN(price)) {
        subtotal += price;
      }
    });
    return subtotal;
  });

  self.subtotal.formatted = ko.computed(function() {
    var val = self.subtotal();

    if(isNaN(val)) {
      return '';
    } else {
      return '$' + val.toFixed(2);
    }
  });

  self._load(data);

  return self;
};
