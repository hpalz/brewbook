  // ========================================================================
  // Ingredient Editors
  // ========================================================================

  self.fermentablePickerView =
    new BT.Recipes.Editor.IngredientPicker({
      batchSize: self.batchSize,
      boundRow: self.currentRecipeFermentable,
      ingredients: ko.observableArray(BT.genericFermentables()),
      modalSelector: "#fermentable-picker-view",
      boundCollection: this.fermentables,
      buildHaystack: function(obj) {
        return obj.name + " " +
               obj.fermentable_type_name + " " +
               obj.ppg + " " +
               obj.color;
      },
      pick: function(pickedObject) {
        var propertyMap = {
          name:              pickedObject.name,
          ppg:               pickedObject.ppg,
          color:             pickedObject.color,
          fermentableTypeId: pickedObject.fermentable_type_id,
          maltsterId:        pickedObject.maltster_id
        };

        if(typeof pickedObject.generic_fermentable_id !== 'undefined') {
          // User picked a custom fermentable
          propertyMap.genericFermentableId = pickedObject.generic_fermentable_id;
          propertyMap.fermentableId = pickedObject.id;
        } else {
          // User picked a generic fermentable
          propertyMap.genericFermentableId =
            pickedObject.id;
          propertyMap.fermentableId = null;
        }

        return propertyMap;
      }
    });

  self.hopPickerView =
    new BT.Recipes.Editor.IngredientPicker({
      boundRow: self.currentRecipeHop,
      ingredients: BT.hops,
      modalSelector: "#hop-picker-view",
      boundCollection: this.hops,
      buildHaystack: function(obj) {
        return obj.name;
      },
      pick: function(pickedObject) {
        return {
          name:      pickedObject.name,
          hopId:     pickedObject.id,
          alpha:     pickedObject.alpha,
          countryId: pickedObject.country_id
        };
      }
    });

  self.yeastPickerView =
    new BT.Recipes.Editor.IngredientPicker({
      boundRow: self.currentRecipeYeast,
      ingredients: BT.yeasts,
      modalSelector: "#yeast-picker-view",
      boundCollection: this.yeasts,
      buildHaystack: function(obj) {
        return obj.name + ' ' + obj.laboratoryId + ' ' + obj.productId;
      },
      pick: function(pickedObject) {
        return {
          name:           pickedObject.name,
          yeastId:        pickedObject.id,
          laboratoryId:   pickedObject.laboratory_id,
          productId:      pickedObject.product_id,
          attenuation:    ((pickedObject.attenuation_min + pickedObject.attenuation_max) / 2),
          yeastFormId:    pickedObject.yeast_form_id,
          yeastTypeId:    pickedObject.yeast_type_id,
          temperatureMin: pickedObject.temperature_min,
          temperatureMax: pickedObject.temperature_max
        };
      }
    });

  self.extraPickerView =
    new BT.Recipes.Editor.IngredientPicker({
      boundRow: self.currentRecipeExtra,
      ingredients: BT.extras,
      modalSelector: "#extra-picker-view",
      boundCollection: this.extras,
      buildHaystack: function(obj) {
        return obj.name;
      },
      pick: function(pickedObject) {
        return {
          name:    pickedObject.name,
          extraId: pickedObject.id
        };
      }
    });


  // ========================================================================
  // Util
  // ========================================================================

  self._scaleIngredients = function(scaleFactor, collection) {
    ko.utils.arrayForEach(collection, function(ingredient) {
      var newAmount = ingredient.amount() * scaleFactor;

      // Round?
      // FIXME: Round based on units
      if(self.roundWhenScaling()) {
        newAmount = newAmount.round(1);
      }

      ingredient.amount(newAmount);
    });
  }

  self._scaleBaseMalts = function(scaleFactor) {
    ko.utils.arrayForEach(self.fermentables(), function(ingredient) {
      if(ingredient.fermentableTypeId() !== BT.Enum.FermentableType.BASE_MALT) {
        return;
      }

      var newAmount = ingredient.amount() * scaleFactor;

      // Round?
      // FIXME: Round based on units
      if(self.roundWhenScaling()) {
        newAmount = newAmount.round(1);
      }

      ingredient.amount(newAmount);
    });
  }

  self.scaleRecipe = function() {
    // Calculate scale factor before changing batch size
    var scaleFactor = self.targetScaledBatchSize() / self.batchSize();

    // Now we can change the batch size
    self.batchSize(self.targetScaledBatchSize());

    // Scale ingredients
    self._scaleIngredients(scaleFactor, self.fermentables());
    self._scaleIngredients(scaleFactor, self.hops());
  }

  self.scaleRecipeForEfficiency = function() {
    // Calculate scale factor before changing batch size
    var scaleFactor = self.efficiency() / self.targetScaledEfficiency();

    // Now we can change the batch size
    self.efficiency(self.targetScaledEfficiency());

    // Scale ingredients
    self._scaleBaseMalts(scaleFactor);
  }


  // ========================================================================
  // Validation
  // ========================================================================

  self.isValid = ko.computed(function() {
    return self.name.isValid()
      && self.kettleVolume.isValid()
      && self.batchSize.isValid()
      && self.trubChillerLoss.isValid()
      && self.boilTime.isValid()
      && self.evapRate.isValid()
      && self.equipmentProfileId.isValid()
      && self.efficiency.isValid();
  });


  // ========================================================================
  // Serialization
  // ========================================================================

  self.toJS = function() {
    function collectValid(array) {
      return ko.utils.arrayMap(
        ko.utils.arrayFilter(array(), function(o) {
          return o.isValid();
        }),
        function(o) {
          return o.toJS();
        }
      );
    }

    var fermentables = collectValid(self.fermentables);
    var hops         = collectValid(self.hops);
    var yeasts       = collectValid(self.yeasts);
    var extras       = collectValid(self.extras);
    var mash_steps   = collectValid(self.mashSteps);

    var obj = {
      name:                  self.name(),
      slug:                  self.slug(),
      is_persisted:          self.isPersisted(),
      parent_id:             self.parentId(),
      status_id:             self.statusId(),
      is_private:            self.isPrivate(),
      measurement_system_id: BT.measurementSystem.measurementSystemId(),
      equipment_profile_id:  self.equipmentProfileId(),
      batch_size:            self.batchSize(),
      kettle_volume:         self.kettleVolume(),
      boil_time:             self.boilTime(),
      evap_rate:             self.evapRate(),
      top_up_water:          self.emulatedRecipe.topUpWater(),
      boil_size:             self.emulatedRecipe.boilSize(),
      recipe_type_id:        self.recipeTypeId(),
      style_id:              self.styleId(),
      efficiency:            self.efficiency(),
      fermentables:          fermentables,
      hops:                  hops,
      yeasts:                yeasts,
      extras:                extras,
      mash_steps:            mash_steps,
      notes:                 self.notes(),
      color:                 self.emulatedRecipe.colorMorey(),
      og:                    self.emulatedRecipe.originalGravity(),
      fg:                    self.emulatedRecipe.finalGravity(),
      ibu:                   self.emulatedRecipe.estimatedIbu(),
      balance:               self.emulatedRecipe.balance(),
      abv:                   self.emulatedRecipe.alcoholByVolume(),
      ibu_formula_id:        self.ibuFormulaId()
    };

    if(self.saveAsGroup() === true) {
      obj.group_id = self.groupId();
    }

    return obj;
  }


  // ========================================================================
  // Constructor
  // ========================================================================

  for(var i = 0; i < BT.equipmentProfiles.length; i++) {
    var e = BT.equipmentProfiles[i];
    self.equipmentProfiles.push(new BT.Recipes.Editor.EquipmentProfile(self, e));
  }

  for(var i = 0; i < attr.recipe_fermentables.length; i++) {
    var f = attr.recipe_fermentables[i];
    self.fermentables.push(new BT.Recipes.Editor.RecipeFermentable(self, f));
  }

  for(var i = 0; i < attr.recipe_hops.length; i++) {
    var h = attr.recipe_hops[i];
    self.hops.push(new BT.Recipes.Editor.RecipeHop(self, h));
  }

  for(var i = 0; i < attr.recipe_yeasts.length; i++) {
    var y = attr.recipe_yeasts[i];
    self.yeasts.push(new BT.Recipes.Editor.RecipeYeast(self, y));
  }

  for(var i = 0; i < attr.recipe_extras.length; i++) {
    var e = attr.recipe_extras[i];
    self.extras.push(new BT.Recipes.Editor.RecipeExtra(self, e));
  }

  for(var i = 0; i < attr.recipe_mash_steps.length; i++) {
    var m = attr.recipe_mash_steps[i];
    self.mashSteps.push(new BT.Recipes.Editor.RecipeMashStep(self, m));
  }

  return self;
};
