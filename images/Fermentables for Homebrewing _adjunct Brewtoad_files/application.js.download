BT.getPropertyByString = function(obj, propString) {
  if(!propString) {
    return obj;
  }

  var prop, props = propString.split(".");

  for(var i = 0, len = props.length - 1; i < len; i++) {
    prop = props[i];

    if(typeof obj === "object" && obj !== null && prop in obj) {
      obj = obj[prop];
    } else {
      break;
    }
  }

  return obj[props[i]];
}

BT.percentOfRange = function(min, max, val) {
