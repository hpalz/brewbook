BT.Shopping.Checkout.Summary.OrderGroupIngredient = function(data) {
  var self = this;

  self.loaded = ko.observable(false);
  self.orderId = ko.observable(data.orderId);
  self.id = ko.observable(data.id);
  self.ingredientId = ko.observable();
  self.ingredientType = ko.observable();
  self.amount = ko.observable();
  self.amountUnit = ko.observable();
  self.constraints = ko.observable();
  self.hopForms = ko.observableArray(data.hopForms);
  self.hops = ko.observableArray(data.hops);
  self.yeasts = ko.observableArray(data.yeasts);
  self.extras = ko.observableArray(data.extras);
  self.fermentables = ko.observableArray(data.fermentables);
  self.vendors = ko.observableArray();
  self.products = ko.observableArray([]);
  self.preferredVendorId = ko.observable();

  self.totalPrice = ko.computed(function() {
    var sum = 0.0;
    ko.utils.arrayForEach(self.products(), function(product) {
      sum += parseFloat(product.total_price);
    });
    return sum.toFixed(2);
  });

  // The template to render given the ingredient type
  // Something like "genericFermentableFields"
  // Returns an empty string if no ingredient type is set.
  self.ingredientTemplateName = ko.computed(function() {
    if(typeof self.ingredientType() !== "undefined")
      return self.ingredientType().charAt(0).toLowerCase() + self.ingredientType().slice(1) + "Fields";
    else
      return "";
  });

  self.totalAmount = ko.computed(function() {
    var sum = 0.0;
    ko.utils.arrayForEach(self.products(), function(product) {
      sum += parseFloat(product.amount);
    });
    return sum;
  });

  self.totalQuantity = ko.computed(function() {
    var sum = 0;
    ko.utils.arrayForEach(self.products(), function(product) {
      sum += parseInt(product.quantity);
    });
    return sum;
  });

  self.productName = ko.computed(function() {
    if(self.products().length > 0)
      return self.products()[0].name;
    else
      return "";
  });

  self.vendorName = ko.computed(function() {
    if(self.products().length > 0)
      return self.products()[0].vendor_name;
    else
      return "";
  });

  self.close = function() {
    self._updateSummary();
    BT.hideModal(".new-modal");

    setTimeout(function() {
      $(".new-modal").remove();
    }, 100);
  };

  self._updateSummary = function() {
    $.ajax("/shopping/orders/" + self.orderId() + "/summary", {
      success: function(data) {
        $(".shopping-order-summary").html(data);
      },
      error: function() {
      }
    });

    $.ajax("/shopping/orders/" + self.orderId() + "/sidebar", {
      success: function(data) {
        $(".shopping-checkout--sidebar table").replaceWith(data);
      },
      error: function() {
      }
    });
  };

  self._updateProducts = function() {
    if(!self.loaded())
      return;

    if(isNaN(self.amount()) || typeof self.amount() === "undefined" || self.amount() < 0 || self.amount() === "")
      return;

    var data = {
      preferred_vendor_id: self.preferredVendorId() || null,
      ingredient_id:       self.ingredientId(),
      amount:              self.amount()
    };

    if(typeof self.constraints() !== "undefined") {
      data = $.extend(data, { constraints: self.constraints().toJS() });
    }

    $.ajax('/shopping/order-group-ingredients/' + self.id(), {
      type: 'PUT',
      data: data,
      success: function(data) {
        self.products.removeAll();
        self.products(data.products);
        self.vendors(data.vendors);
      },
      error: function() {
        // FIXME: Better error handling
        alert('Error!');
      }
    });
  }

  $.ajax("/shopping/order-group-ingredients/" + self.id(), {
    success: function(data) {
      self.ingredientId(data.ingredient_id);
      self.amount(data.amount);
      self.amountUnit(data.amount_unit);
      self.preferredVendorId(data.preferred_vendor_id);
      self.vendors(data.vendors);
      self.products(data.products);
      self.ingredientType(data.ingredient_type);

      if(data.ingredient_type === "GenericFermentable") {
        self.constraints(new BT.Shopping.Checkout.Summary.FermentableConstraints(data.constraints));
      } else if(data.ingredient_type === "Hop") {
        self.constraints(new BT.Shopping.Checkout.Summary.HopConstraints(data.constraints));
      }

      self.loaded(true);
    },
    error: function() {
      alert("Error fetching ingredient data.");
    }
  });

  self.amount.subscribe(function() {
    self._updateProducts();
  });

  self.ingredientId.subscribe(function() {
    self._updateProducts();
  });

  $(".new-modal").on("click", "input[type=checkbox]", function() {
    self._updateProducts();
  });

  ko.applyBindings(self, $('.new-modal .form')[0]);
}

BT.Shopping.Checkout.Summary.Editor = function(data) {
