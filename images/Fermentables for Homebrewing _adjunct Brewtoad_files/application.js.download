BT.Admin.Shopping.ProductEditor.ProductEditor = function(model, data) {
  var self = this;

  self.id             = ko.observable(model.id);
  self.name           = ko.observable(model.name);
  self.vendorId       = ko.observable(model.vendor_id);
  self.sellerId       = ko.observable(model.seller_id);
  self.sku            = ko.observable(model.sku);
  self.taxable        = ko.observable(model.taxable);
  self.backordered    = ko.observable(model.backordered);
  self.ingredientType = ko.observable(model.ingredient_type);
  self.ingredientId   = ko.observable(model.ingredient_id);
  self.amount         = ko.observable(model.amount);
  self.amountUnit     = ko.observable(model.amount_unit);
  self.cost           = ko.observable(model.cost);
  self.shippingWeight = ko.observable(model.shipping_weight);
  self.flags          = ko.observableArray();
  self.options        = null;

  self.ingredients = ko.observableArray();

  if(model.options) {
    switch(self.ingredientType()) {
      case 'GenericFermentable':
        self.options = new BT.Admin.Shopping.ProductEditor.FermentableOptions(model.options);
        break;
      case 'Hop':
        self.options = new BT.Admin.Shopping.ProductEditor.HopOptions(model.options);
        break
      default:
        self.options = null;
    }
  }

  model.flags.forEach(function(flag) {
    self.flags.push(new BT.Admin.Shopping.ProductEditor.Flag(flag));
  });

  self.toJS = function() {
    var flags = [];
    self.flags().forEach(function(flag) {
      flags.push(flag.toJS());
    });

    return {
      id: self.id(),
      name: self.name(),
      vendor_id: self.vendorId(),
      seller_id: self.sellerId(),
      sku: self.sku(),
      taxable: self.taxable(),
      backordered: self.backordered(),
      ingredient_type: self.ingredientType() || null,
      ingredient_id: self.ingredientId() || null,
      amount: self.amount(),
      amount_unit: self.amountUnit(),
      cost: self.cost(),
      shipping_weight: self.shippingWeight(),
      flags: flags
    };
  };

  self.addFlag = function() {
    self.flags.push(new BT.Admin.Shopping.ProductEditor.Flag());
  };

  self.removeFlag = function(which) {
    self.flags.remove(which);
  };

  self.path = function() {
    if(self.id() === null) {
      return '/admin/shopping/products';
    } else {
      return '/admin/shopping/products/' + self.id();
    }
  };

  self.save = function() {
    var data = {shopping_product: self.toJS()};

    if(self.options !== null) {
      data['options'] = self.options.toJS();
    }

    if(self.id() === null) {
      var url = '/admin/shopping/products';
      var type = 'POST';
    } else {
      var url = '/admin/shopping/products/' + self.id();
      var type = 'PUT';
    }

    $.ajax(url, {
      type: type,
      data: data,
      success: function(data) {
        history.replaceState(null, data.name + ' | Brewtoad', '/admin/shopping/products/' + data.id + '/edit');
        $('html,body').animate({ scrollTop: 0 }, 100);

        // Update ID
        self.id(data.id);
      },
      error: function(jqXHR) {
        alert(JSON.stringify(JSON.parse(jqXHR.responseText), null, 2));
      }
    })
  };

  self.updateIngredients = function() {
    self.ingredients.removeAll();

    switch(self.ingredientType()) {
      case 'GenericFermentable':
        var ingredients = data.genericFermentables;
        break;
      case 'Hop':
        var ingredients = data.hops;
        break;
      case 'Yeast':
        var ingredients = data.yeasts;
        break;
      case 'Extra':
        var ingredients = data.extras;
        break;
    }

    if(typeof ingredients !== 'undefined') {
      ingredients.forEach(function(i) {
        self.ingredients.push(i);
      });
    }
  };

  self.ingredientType.subscribe(function() {
    self.ingredientId(null);
    self.updateIngredients();

    switch(self.ingredientType()) {
      case 'GenericFermentable':
        self.options = new BT.Admin.Shopping.ProductEditor.FermentableOptions();
        break;
      case 'Hop':
        self.options = new BT.Admin.Shopping.ProductEditor.HopOptions();
        break
      default:
        self.options = null;
    }
  });

  self.data = data;
  self.data.ingredientTypes = ['GenericFermentable', 'Hop', 'Yeast', 'Extra'];

  self.updateIngredients();

  ko.applyBindings(self, $('#product')[0]);
};
