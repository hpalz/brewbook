$(function() {
  function resetSubmitBtn($addCommentBtn) {
    $addCommentBtn.text("Add Comment").removeClass("red green btn-disabled").text("Add Comment");
  }

  function appendErrorMessage($addCommentBtn, message) {
    $addCommentBtn.removeClass("btn-disabled").addClass("red").text(message);
  }

  function appendSuccessMessage($addCommentBtn, message) {
    $addCommentBtn.removeClass("btn-disabled").addClass("green").text(message);
  }

  $(".js-recipe-comments").on("keyup", "textarea", function() {
    resetSubmitBtn($(this).next());
  });

  $(".js-recipe-comments").on("click", ".js-submit-comment", function(e) {
    e.preventDefault();

    var $this = $(this);
    var $form = $this.parents(".comment-form").first();
    var comment = $form.find("textarea").val();
    var parent_id = $this.attr("data-reply-to") || undefined;

    if($this.hasClass("btn-disabled")) {
      return false;
    }

    if($.trim(comment) === "") {
      appendErrorMessage($this, "Write something!");
      return false;
    }

    $this.addClass("btn-disabled").text("Submitting Comment...");

    var ajax = $.ajax({
      async: true,
      data: {
        comment: {
          body: comment,
          parent_id: parent_id
        }
      },
      type: "POST",
      url: $(this).attr("data-path") + "/comments",
      success: function(r) {
        $form.find("textarea").val("");
        appendSuccessMessage($this, "Comment Posted!");

        var $r = $(r);
        if(typeof parent_id === "undefined") {
          $("#js-comment-list-all").append($r);
        } else {
          $form.parents(".recipe-comment").first().find(".comment-replies").first().addClass("in").append($r);
          // Automatically hide the comment form for replies
          $form.hide();
        }

        // Scroll to show where it was just added
        $("html,body").animate({
          scrollTop: $r.offset().top
        }, 750, function() {
          $r.removeClass("new-comment");
        });
      },
      error: function(r) {
        alert("Error submitting comment.");
      }
    });
  });

  $('.js-recipe-comments').on('click', '.show-reply-form', function(e) {
    $(this).closest('.recipe-comment').find('.comment-form').first().toggle();
    e.preventDefault();
  });
});


