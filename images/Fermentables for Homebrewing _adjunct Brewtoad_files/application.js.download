BT.Calculators.MashInfusion = function(args) {
  var self = this;

  self.args = args;

  self.startingTemp  = ko.measuredValueObservable(152, 'degF',  'degC');
  self.grainWeight   = ko.measuredValueObservable(10,  'lb',    'kg');
  self.mashThickness = ko.measuredValueObservable(1.5, 'qt/lb', 'L/kg');
  self.targetTemp    = ko.measuredValueObservable(168, 'degF',  'degC');

  self.thermodynamicConstant = ko.computed(function() {
    if(BT.measurementSystem.isMetric()) {
      return 0.41;
    } else {
      return 0.2;
    }
  });

  self.mashVolume = ko.computed(function() {
    return self.grainWeight() * self.mashThickness();
  });

  self.volumeAddition = ko.computed(function() {
    if(self.targetTemp() <= self.startingTemp()) {
      return 0;
    }
    var normalizedVolumeAddition = ((self.targetTemp() - self.startingTemp()) *
      (self.thermodynamicConstant() * self.grainWeight() + self.mashVolume()) /
      (BT.measurementSystem.boilingTemp() - self.targetTemp()));
    if(isNaN(normalizedVolumeAddition)) {
      return 0;
    }
    return normalizedVolumeAddition;
  });

  //
  // Bind
  //

  ko.applyBindings(
    self,
    $(self.args.bindTarget)[0]
  );
};
function MashWaterCalculator(attrs) {
  for(var i in attrs) {
    this[i] = attrs[i];
  }

  if(this.metric) {
    this.thermodynamicConstant = 0.41;
    this.mashOutTemp = 75.5;
    this.volumeUnit = 'L'
    this.weightUnit = 'kg'
    this.tempUnit   = '&deg;C';
  } else {
    this.thermodynamicConstant = 0.2;
    this.mashOutTemp = 168;
    this.volumeUnit = 'gal'
    this.weightUnit = 'lb'
    this.tempUnit   = '&deg;F';
  }

  this.strikeStep = function(mashWater, strikeTemp) {
    return 'Strike grains with {mashWater} {volumeUnit} of water at {strikeTemp} {tempUnit}.'.supplant({
      mashWater:  mashWater.round(2),
      volumeUnit: this.volumeUnit,
      strikeTemp: strikeTemp.round(1),
      tempUnit:   this.tempUnit
    });
  }

  this.mashRestStep = function(mashTemp, mashTime) {
    return 'Mash at {mashTemp} {tempUnit} for {mashTime} min.'.supplant({
      mashTemp: mashTemp.round(1),
      tempUnit: this.tempUnit,
      mashTime: mashTime
    });
  }

  this.vorlaufAndLauterStep = function(runnings, which) {
    return 'Vorlauf and lauter {runnings} {volumeUnit} in your {which} runnings.'.supplant({
      runnings:   runnings.round(2),
      volumeUnit: this.volumeUnit,
      which:      which
    });
  }

  this.spargeStep = function(spargeWater, spargeTemp) {
    return 'Add {spargeWater} {volumeUnit} of sparge water at {spargeTemp} {tempUnit}.'.supplant({
      spargeWater: spargeWater.round(2),
      spargeTemp:  spargeTemp,
      volumeUnit:  this.volumeUnit,
      tempUnit:    this.tempUnit
    });
  }

  this.mashOutStep = function(mashOut, mashOutTemp) {
    return 'Mash out with {mashOut} {volumeUnit} of water at {mashOutTemp} {tempUnit}.'.supplant({
      mashOut:     mashOut.round(2),
      volumeUnit:  this.volumeUnit,
      mashOutTemp: mashOutTemp,
      tempUnit:    this.tempUnit
    });
  }

  this.combinedRunningsStep = function(combinedRunnings) {
    return 'Your combined runnings should be {combinedRunnings} {volumeUnit}.'.supplant({
      combinedRunnings: combinedRunnings.round(2),
      volumeUnit:       this.volumeUnit
    });
  }

  this.rinseSpargeStep = function(spargeWaterVolume) {
    return 'Rinse the grain in {spargeWaterVolume} {volumeUnit} sparge water at {spargeTemp} {tempUnit}.'.supplant({
      spargeWaterVolume: spargeWaterVolume.round(2),
      spargeTemp:        this.mashOutTemp,
      volumeUnit:        this.volumeUnit,
      tempUnit:          this.tempUnit
    });
  }

  this.drainBagStep = function(combinedRunningsVolume) {
    return 'Drain the grain bag into the mash water to make {combinedRunningsVolume} {volumeUnit} wort.'.supplant({
      combinedRunningsVolume: combinedRunningsVolume.round(2),
      volumeUnit:             this.volumeUnit
    });
  }

  this.drainBagAndCombineRunningsStep = function(combinedRunningsVolume) {
    return 'Drain the grain bag and combine the mash and sparge water to make {combinedRunningsVolume} {volumeUnit} wort.'.supplant({
      combinedRunningsVolume: combinedRunningsVolume.round(2),
      volumeUnit:             this.volumeUnit
    });
  }

  //
  // Returns the temperature of water required to raise the grain to the
  // correct mash temperature
  //
  this.strikeWaterTemp = function(mashThickness, mashTemp, grainTemp) {
    if(this.metric) {
      var mt = (mashThickness * 0.119826427) * 4;
    } else {
      var mt = mashThickness * 4;
    }
    return (this.thermodynamicConstant / mt) *
      (mashTemp - grainTemp) + mashTemp;
  }

  //
  // Returns the volume in gallons of boiling water required to raise the
  // mash to a given temperature
  //
  this.mashInfusionTemp = function(mashVolume, grainWeight, mashTemp, targetTemp) {
    return ((targetTemp - mashTemp) *
      (this.thermodynamicConstant * grainWeight + mashVolume) /
      (this.boilingTemp - targetTemp)) / 4;
  }

  //
  // Returns the volume in gallons of boiling water required to raise the mash
  // to the mash out temperature
  //
  this.mashOutWaterVolume = function(mashVolume, grainWeight, mashTemp) {
    return this.mashInfusionTemp(mashVolume, grainWeight, mashTemp, this.mashOutTemp);
  }

  //
  // Wraps each list item in HTML list item tags, adds line breaks, and returns
  // a string
  //
  this.arrayToList = function(arr) {
    return arr.map(function(o) { return '<li>' + o + '</li>' }).join("\n");
  }

  //
  // A single infusion mash with a single batch sparge
  //
  this.singleInfusionBatchSparge = function() {
    //
    var mashWaterVolume = this.grainWeight * this.mashThickness + this.lauterDeadspace;

    //
    var correctedMashThickness = mashWaterVolume / this.grainWeight;

    //
    var spargeWaterVolume = this.boilVolume - mashWaterVolume +
      (this.grainWeight * this.grainAbsorptionFactor) + this.lauterDeadspace;

    //
    var firstRunnings = mashWaterVolume -
      (this.grainWeight * this.grainAbsorptionFactor) - this.lauterDeadspace;

    //
    var secondRunnings = spargeWaterVolume;

    //
    var combinedRunnings = firstRunnings + secondRunnings;

    //
    var strikeWaterTemp =
      this.strikeWaterTemp(correctedMashThickness, this.mashTemp, this.grainTemp);

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.vorlaufAndLauterStep(firstRunnings, 'first'),
      this.spargeStep(spargeWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(secondRunnings, 'second'),
      this.combinedRunningsStep(combinedRunnings)
    ]);
  }

  //
  // A single infusion mash with a mash out raise the mash out temp before
  // a single batch sparge
  //
  this.singleInfusionMashOutBatchSparge = function() {
    // Water required to mash plus additional water to fill the mash tun
    var mashWaterVolume = (this.grainWeight * this.mashThickness) +
      this.lauterDeadspace;

    // Mash thickness changes slightly because of the lauter deadspace
    var correctedMashThickness = mashWaterVolume / this.grainWeight;

    // Calculate amount of boiling water is needed to bring the mash to the mash
    // out temperature
    var mashOutWaterVolume =
      this.mashOutWaterVolume(mashWaterVolume, this.grainWeight, this.mashTemp);

    // Mash water + mash out water - water absorbed by grain - lauter deadspace
    var firstRunnings = mashWaterVolume + mashOutWaterVolume -
      (this.grainWeight * this.grainAbsorptionFactor) - this.lauterDeadspace;

    // Use the rest of the water for the sparge
    var spargeWaterVolume = this.boilVolume - firstRunnings;

    // Grain is already saturated and deadspace is full
    var secondRunnings = spargeWaterVolume;

    // Combined runnings
    var combinedRunnings = firstRunnings + secondRunnings;

    // Calculate strike water temperature
    var strikeWaterTemp =
      this.strikeWaterTemp(correctedMashThickness, this.mashTemp, this.grainTemp);

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.mashOutStep(mashOutWaterVolume, this.boilingTemp),
      this.vorlaufAndLauterStep(firstRunnings, 'first'),
      this.spargeStep(spargeWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(secondRunnings, 'second'),
      this.combinedRunningsStep(combinedRunnings)
    ]);
  }

  //
  // A single infusion mash with a mash out to bring the volume of the first
  // runnings to half the boil volume and a single batch sparge with the
  // remaining water.
  //
  this.singleInfusionMashOutBatchSpargeEqualRunnings = function() {
    // Mash with just enough water
    var mashWaterVolume = this.grainWeight * this.mashThickness;

    // Mash out water volume + first runnings = half of the pre-boil volume
    var mashOutWaterVolume = (this.boilVolume / 2) -
      (mashWaterVolume - (this.grainWeight * this.grainAbsorptionFactor) - this.lauterDeadspace);

    // Sparge water makes up the other half of the pre-boil volume
    var spargeWaterVolume = this.boilVolume / 2;

    // Mash water + mash out water - grain absorption - lauter deadspace
    var firstRunnings = mashWaterVolume + mashOutWaterVolume -
      (this.grainWeight * this.grainAbsorptionFactor) - this.lauterDeadspace;

    //
    var secondRunnings = spargeWaterVolume;

    //
    var combinedRunnings = firstRunnings + secondRunnings;

    //
    var strikeWaterTemp =
      this.strikeWaterTemp(this.mashThickness, this.mashTemp, this.grainTemp);

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.mashOutStep(mashOutWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(firstRunnings, 'first'),
      this.spargeStep(spargeWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(secondRunnings, 'second'),
      this.combinedRunningsStep(combinedRunnings)
    ]);
  }

  //
  // A single infusion mash with two batch sparges, each using half of the total
  // sparge water.
  //
  this.singleInfusionTwoEqualBatchSparges = function() {
    //
    var mashWaterVolume = this.grainWeight * this.mashThickness + this.lauterDeadspace;

    //
    var correctedMashThickness = mashWaterVolume / this.grainWeight;

    //
    var firstRunnings = mashWaterVolume -
      (this.grainWeight * this.grainAbsorptionFactor) - this.lauterDeadspace;

    //
    var spargeWaterVolume = (this.boilVolume - firstRunnings) / 2;

    //
    var secondRunnings = spargeWaterVolume;

    //
    var thirdRunnings = spargeWaterVolume;

    //
    var combinedRunnings = firstRunnings + secondRunnings + thirdRunnings;

    //
    var strikeWaterTemp =
      this.strikeWaterTemp(correctedMashThickness, this.mashTemp, this.grainTemp);

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.vorlaufAndLauterStep(firstRunnings, 'first'),
      this.spargeStep(spargeWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(secondRunnings, 'second'),
      this.spargeStep(spargeWaterVolume, this.mashOutTemp),
      this.vorlaufAndLauterStep(thirdRunnings, 'third'),
      this.combinedRunningsStep(combinedRunnings)
    ]);
  }

  //
  // A single infusion BIAB mash
  //
  this.singleInfusionNoSpargeBiab = function() {
    // Mash with just enough water
    var mashWaterVolume = this.boilVolume + (this.grainWeight * this.grainAbsorptionFactor);

    //
    var correctedMashThickness = mashWaterVolume / this.grainWeight;

    //
    var combinedRunningsVolume = mashWaterVolume - (this.grainWeight * this.grainAbsorptionFactor);

    //
    var strikeWaterTemp =
      this.strikeWaterTemp(correctedMashThickness, this.mashTemp, this.grainTemp);

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.drainBagStep(combinedRunningsVolume)
    ]);
  }

  //
  // A single infusion BIAB mash with a sparge rinse
  //
  this.singleInfusionSpargeRinseBiab = function() {
    // Mash with just enough water
    var mashWaterVolume = (this.grainWeight * this.mashThickness);

    //
    var strikeWaterTemp =
      this.strikeWaterTemp(this.mashThickness, this.mashTemp, this.grainTemp);

    //
    var spargeWaterVolume = this.boilVolume - mashWaterVolume + (this.grainWeight * this.grainAbsorptionFactor)

    //
    var combinedRunningsVolume = mashWaterVolume + spargeWaterVolume - (this.grainWeight * this.grainAbsorptionFactor)

    return this.arrayToList([
      this.strikeStep(mashWaterVolume, strikeWaterTemp),
      this.mashRestStep(this.mashTemp, this.mashTime),
      this.rinseSpargeStep(spargeWaterVolume),
      this.drainBagAndCombineRunningsStep(combinedRunningsVolume)
    ]);
  }
}
;
BT.Calculators.PitchRate = function(bindTarget) {
  var self = this;

  self.yeastTypes = [
    {
      name: 'Ale',
      cells: 750000
    },
    {
      name: 'Lager',
      cells: 1500000
    }
  ];

  self.measuredOg = ko.floatObservable();
  self.wortVolume = ko.measuredValueObservable(5.5, 'gal', 'L');
  self.cellsForYeastType = ko.intObservable();

  self.numCells = ko.computed(function() {
    var wortVolumeMl = BT.measurementSystem.convert(self.wortVolume.normalized(), 'gal', 'ml');
    var measuredPlato = 259 - (259 / self.measuredOg());
    var numCells = parseFloat(self.cellsForYeastType() * wortVolumeMl * measuredPlato) * 0.000000001;
    if(isNaN(numCells)) {
      return undefined;
    } else {
      return parseInt(numCells);
    }
  });

  self.numLiquid = ko.computed(function() {
    var ret = Math.round(self.numCells() / 100);
    if(isNaN(ret)) {
      return undefined;
    } else {
      return ret;
    }
  });

  self.gramsDry = ko.computed(function() {
    var ret = Math.round(self.numCells() / 20);
    if(isNaN(ret)) {
      return undefined;
    } else {
      return ret;
    }
  });

  //
  // Bind
  //

  if(typeof bindTarget !== 'undefined') {
    ko.applyBindings(
      self,
      $(bindTarget)[0]
    );
  }
};
BT.Recipes.Favorite = function(slug) {
  var self = $('.js-favorite-recipe');

  //
  // Changes the class of the element and manages GA events
  //
  function changeClassText(){
    self.toggleClass('save--link__active');
  }

  //
  // Prost! the recipe
  //
  function save(action) {
    $.ajax({
      type: 'POST',
      url: '/recipes/' + slug + '/' + action + '.json',
      success: function(r) {
        changeClassText();
      },
      error: function(r) {
        if(r.status !== 200) {
          self.removeClass('active');
          self.html('Error!');
        }
      }
    });
  }

  //
  // Sets up the event handlers
  //

  function initUIBindings(slug) {

    self.on('click', function(e){
      e.preventDefault();
      if(self.hasClass('save--link__active')) {
        save('unfavorite');
      } else {
        save('favorite');
      }
    });
  }

  initUIBindings(slug);
};
BT.Recipes.Editor.IngredientPicker = function(args) {
  var self = this;

  self.isEdit = ko.observable(false);
  self.args = args;

  //
  // ========================================================================
  // Properties
  // ========================================================================
  //

  self.searchKeywords        = ko.stringObservable();

  self.filtered = ko.computed(function() {
    if(!self.searchKeywords.isValid()) {
      var searchKeywords = "";
    } else {
      var searchKeywords = self.searchKeywords().toLowerCase().split(" ");
    }

    var matchedIngredients = [];

    ko.utils.arrayForEach(self.args.ingredients(), function(item) {
      var numKeywordMatches = 0;
      var haystack = self.args.buildHaystack(item).toLowerCase();
      for(var i = 0; i < searchKeywords.length; i++) {
        if(haystack.indexOf(searchKeywords[i]) !== -1) {
          numKeywordMatches += 1;
        }
      }
      if(numKeywordMatches === searchKeywords.length) {
        matchedIngredients.push(item);
      }
    });

    return matchedIngredients;
  });

  //
  // ========================================================================
  // Instance Methods
  // ========================================================================
  //

  self.usageRateForBatch = function(ozPerGal) {
    if(BT.measurementSystem.measurementSystemId() === BT.Enum.MeasurementSystem.METRIC) {
      var gPerL = BT.measurementSystem.convert(ozPerGal, 'oz/gal', 'g/L');
      var g = gPerL * self.args.batchSize();
      if(g >= 1000) {
        return (g / 1000).round(1) + ' kg';
      } else {
        return g.round(0) + ' g';
      }
    } else {
      oz = ozPerGal * self.args.batchSize();
      if(oz >= 16) {
        return (oz / 16).round(1) + ' lb';
      } else {
        return oz.round(1) + ' oz';
      }
    }
  }

  self.show = function(isEdit, onPick) {
    self.isEdit(isEdit);
    BT.showModal(self.args.modalSelector);
    $(self.args.modalSelector).find('input').focus();
    if(!isEdit) {
      self.onPick = onPick;
    }
  }

  self.close = function() {
    self.searchKeywords("");
    BT.hideModal(self.args.modalSelector);
  }

  self.pick = function(ingredient) {
    var propertyMap = self.args.pick(ingredient);

    for(var key in propertyMap) {
      var val = propertyMap[key];
      self.boundRow()[key](val);
    }

    if(self.boundRow().name.isValid() && !self.isEdit()) {
      self.args.boundCollection.push(self.boundRow());
      if(typeof self.onPick !== 'undefined') {
        self.onPick();
      }
    }

    // Get the clicked button
    var $btn = $(self.args.modalSelector).find('[data-ingredient-id=' + ingredient.id + ']');

    // Change the icon to a check mark
    $btn.addClass('added');

    // Remove the check mark icon after a delay
    setTimeout(function() {
      $btn.removeClass('added');
    }, 1000);

    // Automatically close the modal
    if(self.isEdit()) {
      setTimeout(function() {
        self.close();
      }, 250);
    }
  }

  //
  // ========================================================================
  // Bind
  // ========================================================================
  //

  self.boundRow = self.args.boundRow;

  $(document).bind('keyup', self.args.modalSelector, function(e) {
    if(e.keyCode === 27) {
      self.close();
    }
  });

  $('.modal-close-target').on('click', function(e) {
    self.close();
  });

  ko.applyBindings(
    self,
    $(self.args.modalSelector)[0]
  );
};
BT.Recipes.Editor.RangeGraph = function(graphMin, graphMax, rangeMin, rangeMax, value) {
  var self = this;

  self.rangeMin     = rangeMin;
  self.rangeMax     = rangeMax;
  self.graphMin     = graphMin;
  self.graphMax     = graphMax;
  self.value        = value;

  self.percentOfRange = function(val) {
    var lowerBound = self.graphMin;//Math.min.apply(null, [self.rangeMin(), self.value()]) - self.graphPadding();
    var upperBound = self.graphMax;//Math.max.apply(null, [self.rangeMax(), self.value()]) + self.graphPadding();

    var percent = BT.percentOfRange(lowerBound, upperBound, val);

    if(percent > 100) {
      return 100;
    } else if(percent < 0) {
      return 0;
    } else {
      return percent
    }
  }

  self.graphRangeMin = ko.computed(function() {
    return self.percentOfRange(self.rangeMin());
  });

  self.graphRangeMax = ko.computed(function() {
    return self.percentOfRange(self.rangeMax());
  });

  self.valueOffset = ko.computed(function() {
    return self.percentOfRange(self.value());
  });

  return self;
};
BT.Recipes.Editor.Recipe = function(attr) {
  var self = this;

  // ========================================================================
  // Properties
  // ========================================================================

  // Header
  self.name                       = ko.stringObservable(attr.name);
  self.recipeTypeId               = ko.intObservable(attr.recipe_type_id);

  // Equipment Profile
  self.equipmentProfileId         = ko.intObservable(attr.equipment_profile_id);

  // Status
  self.statusId                   = ko.intObservable(attr.status_id);
  self.groupId                    = ko.intObservable(attr.group_id);
  self.saveAsGroup                = ko.observable(attr.group_id !== null);
  self.isPrivate                  = ko.observable(attr.is_private);

  // Batch & Boil
  self.kettleVolume               = ko.measuredValueObservable(attr.kettle_volume, "gal", "L");
  self.efficiency                 = ko.floatObservable(attr.efficiency);
  self.batchSize                  = ko.measuredValueObservable(attr.batch_size, "gal", "L");
  self.evapRate                   = ko.floatObservable(attr.evap_rate);
  self.boilTime                   = ko.intObservable(attr.boil_time);
  //self.topUpWater                 = ko.measuredValueObservable(attr.top_up_water_volume, "gal", "L");
  self.trubChillerLoss            = ko.measuredValueObservable(attr.trub_chiller_loss, "gal", "L");

  // Notes
  self.notes                      = ko.observable(attr.notes);

  // Meta
  self.isPersisted                = ko.observable(attr.id !== null);
  self.hasActivity                = ko.observable(attr.has_activity);
  self.parentId                   = ko.intObservable(attr.parent_id);
  self.slug                       = ko.stringObservable(attr.slug)
  self.ibuFormulaId               = ko.intObservable(attr.ibu_formula_id);

  // Validation
  self.validationErrors           = ko.observableArray();

  // Equipment Profiles
  self.equipmentProfiles          = ko.observableArray();

  // Ingredients
  self.fermentables               = ko.observableArray();
  self.hops                       = ko.observableArray();
  self.yeasts                     = ko.observableArray();
  self.extras                     = ko.observableArray();
  self.mashSteps                  = ko.observableArray();

  // Refs for ingredient pickers
  self.currentRecipeFermentable   = ko.observable(null);
  self.currentRecipeHop           = ko.observable(null);
  self.currentRecipeYeast         = ko.observable(null);
  self.currentRecipeExtra         = ko.observable(null);

  // Recipe scaling
  self.targetScaledBatchSize      = ko.measuredValueObservable(attr.batch_size, "gal", "L");
  self.roundWhenScaling           = ko.observable(true)

  self.targetScaledEfficiency     = ko.measuredValueObservable(attr.efficiency, "gal", "L");
  self.roundWhenScalingEfficiency = ko.observable(true)

  // ========================================================================
  // Update recipe when equipment profile changes
  // ========================================================================

  self.equipmentProfileId.subscribe(function(newValue) {
    var equipmentProfile = BT.equipmentProfiles.find(newValue);

    if(typeof equipmentProfile === "undefined") {
      return;
    }

    if(!confirm("Update recipe?")) {
      return;
    }

    // Set measurement system
    BT.measurementSystem.measurementSystemId(equipmentProfile.measurement_system_id);

    self.kettleVolume(equipmentProfile.kettle_volume);
    self.boilTime(equipmentProfile.boil_time);
    self.evapRate(equipmentProfile.evap_rate);
    self.trubChillerLoss(equipmentProfile.trub_chiller_loss);

    if(self.batchSize() !== equipmentProfile.batch_size) {
      self.targetScaledBatchSize(equipmentProfile.batch_size);
      self.scaleRecipe();
    }

    if(self.efficiency() !== equipmentProfile.efficiency) {
      self.targetScaledEfficiency(equipmentProfile.efficiency);
      self.scaleRecipeForEfficiency();
    }
  });


  // ========================================================================
  // Recalculate fermentable uses when recipe type changes
  // ========================================================================

  self.recipeTypeId.subscribe(function(newValue) {
    if(newValue === BT.Enum.RecipeType.EXTRACT) {
      self.efficiency(75);
    } else {
      if(self.efficiency.isInvalid) {
        var equipmentProfile = BT.equipmentProfiles.find(self.equipmentProfileId());
        if(typeof equipmentProfile !== "undefined") {
          self.efficiency(equipmentProfile.efficiency);
        } else {
          self.efficiency(75);
        }
      }
    }

    ko.utils.arrayForEach(self.fermentables(), function(ingredient) {
      ingredient.recalculateFermentableUse();
    });
  });


  // ========================================================================
  // Reload fermentables when the style ID changes
  // ========================================================================

  self._styleId = ko.intObservable(attr.style_id);
  self.styleId = ko.computed({
    read: function() {
      return self._styleId();
    },
    write: function(val) {
      self._styleId(val);

      // Load fermentables for this style
      if(typeof val !== 'undefined') {
        $.getJSON('/generic-fermentables', {style_id: val},
          function(data, textStatus, jqXHR) {
            BT.genericFermentables(data);
          }
        );
      }
    }
  });
  self.styleId.isValid = function() {
    return self._styleId.isValid();
  }


  // ========================================================================
  // Misc. accessors
  // ========================================================================

  self.groupName = ko.computed(function() {
    var group = BT.availableGroups.find(self.groupId());
    if(typeof group === "undefined") {
      return "";
    } else {
      return group.name;
    }
  });

  self.recipeTypeName = ko.computed(function() {
    var type = BT.recipeTypes.find(self.recipeTypeId());
    if(typeof type === "undefined") {
      return "";
    } else {
      return type.name;
    }
  });

  self.styleName = ko.computed(function() {
    var type = BT.styles.find(self.styleId());
    if(typeof type === "undefined") {
      return "";
    } else {
      return type.name;
    }
  });

  self.publishStatus = ko.computed(function() {
    return self.isPublishing;
  });


  // ========================================================================
  // Emulated Recipe
  // ========================================================================

  self.emulatedRecipe = new BT.EmulatedRecipe({
    ibuFormulaId: self.ibuFormulaId,
    fermentables: self.fermentables,
    hops:         self.hops,
    yeasts:       self.yeasts,
    recipeTypeId: self.recipeTypeId,
    batchSize:    self.batchSize,
    //topUpWater:   self.topUpWater,
    evapRate:     self.evapRate,
    boilTime:     self.boilTime,
    efficiency:   self.efficiency,
    kettleVolume: self.kettleVolume,
    trubChillerLoss: self.trubChillerLoss
  });


  // ========================================================================
  // Recipe Style
  // ========================================================================

  self.style = new BT.Recipes.Editor.RecipeStyle(self.styleId);


  // ========================================================================
  // Graphs
  // ========================================================================

  self.styleOgGraph = new BT.Recipes.Editor.RangeGraph(
    1.000,
    1.100,
    self.style.ogMin,
    self.style.ogMax,
    self.emulatedRecipe.originalGravity,
    0.0075
  );

  self.styleFgGraph = new BT.Recipes.Editor.RangeGraph(
    1.000,
    1.110,
    self.style.fgMin,
    self.style.fgMax,
    self.emulatedRecipe.finalGravity,
    0.0075
  );

